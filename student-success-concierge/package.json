  "description": "Teaching app for AI agent evaluation and analysis",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "setup": "npm install && npm run init:db && echo 'Setup complete! Run: npm run dev'",
    "init:db": "tsx scripts/init_dbs.ts",
    "reset:db": "rm -rf data/*.db && npm run init:db",
    "demo:trace": "tsx scripts/create_demo_trace.ts",
    "seed:traces": "tsx scripts/seed_traces.ts",
    "seed:kb": "tsx scripts/seed_kb_articles.ts",
    "seed:learning": "tsx scripts/seed_learning_outcomes.ts",
    "run:evals": "tsx scripts/run_evals.ts",
    "test:labels": "tsx scripts/test_labels.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:phoenix": "docker-compose -f docker-compose.phoenix.yml up -d"
  },
